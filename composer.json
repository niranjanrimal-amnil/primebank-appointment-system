{
    "name": "primebank/appointment-system",
    "description": "Appointment booking system with OTP verification and external API integration",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
           "AppointmentSystem\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppointmentSystem\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AppointmentSystem\\AppointmentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}